<assembly xmlns="http://maven.apache.org/ASSEMBLY/2.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://maven.apache.org/ASSEMBLY/2.0.0 http://maven.apache.org/xsd/assembly-2.0.0.xsd">
    <id>jdk13</id>
    <formats>
        <format>zip</format>
    </formats>
    <fileSets>
        <fileSet>
          <directory>conf/java13/security</directory>
          <outputDirectory>modules_conf/org.openjsse/security</outputDirectory>
          <includes>
            <include>openjsse.security</include>
          </includes>
        </fileSet>
        <fileSet>
          <directory>conf/java13/security</directory>
          <outputDirectory>modules_conf/java.base/security</outputDirectory>
          <includes>
            <include>java.policy.extra</include>
          </includes>
        </fileSet>
        <fileSet>
          <directory>conf/java13</directory>
          <outputDirectory>modules_src/java.base</outputDirectory>
          <includes>
            <include>module-info.java.extra</include>
          </includes>
        </fileSet>
        <fileSet>
          <directory>src/main/resources</directory>
          <outputDirectory>modules_src/org.openjsse</outputDirectory>
          <includes>
            <include>**/*</include>
          </includes>
        </fileSet>
        <fileSet>
          <directory>src/main/java</directory>
          <outputDirectory>modules_src/org.openjsse</outputDirectory>
          <includes>
            <include>**/*</include>
          </includes>
        </fileSet>
        <fileSet>
          <directory>src/main/java13</directory>
          <outputDirectory>modules_src/org.openjsse</outputDirectory>
          <includes>
            <include>**/*</include>
          </includes>
        </fileSet>
        <fileSet>
            <directory>conf/java13</directory>
            <outputDirectory>make/org.openjsse</outputDirectory>
            <includes>
                <include>build.properties</include>
            </includes>
        </fileSet>
    </fileSets>
</assembly>
