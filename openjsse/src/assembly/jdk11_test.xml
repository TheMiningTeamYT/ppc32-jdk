<assembly xmlns="http://maven.apache.org/ASSEMBLY/2.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://maven.apache.org/ASSEMBLY/2.0.0 http://maven.apache.org/xsd/assembly-2.0.0.xsd">
    <id>jdk11_test</id>
    <formats>
        <format>jar</format>
    </formats>
    <fileSets>
        <fileSet>
          <directory>test/java11/org</directory>
          <outputDirectory>test/jdk/org</outputDirectory>
          <includes>
            <include>**/*</include>
          </includes>
        </fileSet>
        <fileSet>
          <directory>test/java11/lib</directory>
          <outputDirectory>test/jdk/lib</outputDirectory>
          <includes>
            <include>**/*</include>
          </includes>
        </fileSet>
    </fileSets>
</assembly>
